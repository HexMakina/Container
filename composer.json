{
    "name": "hexmakina/le-marchand",
    "description": "LeMarchand is a PSR-11 container, locator and dependency injector",
    "keywords": [
        "container",
        "psr-11",
        "psr11"
    ],
    "type": "package",
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "license": "mit",
    "authors": [
        {
            "name": "HexMakina",
            "email": "touch@hexmakina.be",
            "homepage": "https://hexmakina.be"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "HexMakina\\LeMarchand\\": "./src"
        }
    }
}